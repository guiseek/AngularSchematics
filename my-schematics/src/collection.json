{
  "$schema": "../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "my-schematics": {
      "description": "A blank schematic.",
      "factory": "./my-schematics/index#mySchematics"
    },
    "my-plugin": {
      "description": "Plugin schematic.",
      "factory": "./my-plugin/index#myPlugin",
      "schema": "./my-plugin/schema.json"
    },
    "my-carousel": {
      "description": "Carousel schematic.",
      "factory": "./my-carousel/index#myCarousel",
      "schema": "./my-carousel/schema.json"
    }
  }
}
